{
  "name": "@pixelretail/slideshow",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "test": "yarn eslint",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js && yarn prettier-html",
    "prettier": "prettier --write './{src,config}/**/*.{js,md,twig}' && yarn prettier-stylus && yarn prettier-twig",
    "prettier-twig": "prettier --write '../templates/**/*.twig' --twig-print-width=200",
    "prettier-stylus": "stylus-supremacy format './src/style/**/*.styl' --options ./config/.stylus.json --replace",
    "prettier-html": "prettyhtml --printWidth 130 './dist/**/*.html'",
    "prettier-html-src": "prettyhtml --printWidth 130 './src/**/*.html'",
    "eslint": "eslint src config --ext .js -c config/eslint.config.js",
    "precommit": "yarn build && yarn prettier && git add ../templates && git add ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "author": "",
  "license": "ISC",
